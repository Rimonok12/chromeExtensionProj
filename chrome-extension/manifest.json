{
    "manifest_version": 2,
    "name": "MERN Chrome Extension",
    "description": "A Chrome Extension using MERN Stack",
    "version": "1.0",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "icon.png"
    },
    "permissions": ["activeTab"]
}
